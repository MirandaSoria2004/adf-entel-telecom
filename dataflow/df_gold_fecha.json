{
	"name": "df_gold_fecha",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_llamadas",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_datos",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          fecha as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT CAST(fecha AS nvarchar) AS fecha\\nFROM silver.llamadas\\nWHERE fecha IS NOT NULL',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          fecha as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT CAST(fecha_inicio AS datetime) AS fecha\\nFROM silver.datos\\nWHERE fecha_inicio IS NOT NULL\\nUNION\\nSELECT DISTINCT CAST(fecha_fin AS datetime) AS fecha\\nFROM silver.datos\\nWHERE fecha_fin IS NOT NULL',",
				"     format: 'query') ~> source2"
			]
		}
	}
}